local function getBundle(name)
    return bundles[name] or bundles["Ninja"]
end

local finalData = {}

if _G.HybridMode then
    finalData["idle"] = getBundle(_G.H_Idle).idle
    finalData["walk"] = getBundle(_G.H_Walk).walk
    finalData["run"] = getBundle(_G.H_Run).run
    finalData["jump"] = getBundle(_G.H_Jump).jump
    finalData["fall"] = getBundle(_G.H_Fall).fall
    finalData["climb"] = getBundle(_G.H_Climb).climb
    finalData["swim"] = getBundle(_G.H_Swim).swim
else
    finalData = getBundle(_G.SelectedAnim)
end

local hum = char:FindFirstChildOfClass("Humanoid")
local animate = char:FindFirstChild("Animate")

if animate and hum then
    for _, v in pairs(hum:GetPlayingAnimationTracks()) do v:Stop(0) end

    for folderName, id in pairs(finalData) do
        local folder = animate:FindFirstChild(folderName)
        if folder then
            for _, animObj in pairs(folder:GetChildren()) do
                if animObj:IsA("Animation") then
                    animObj.AnimationId = "rbxassetid://" .. tostring(id):gsub("rbxassetid://", "")
                end
            end
        end
    end

    animate.Disabled = true
    task.wait(0.1)
    animate.Disabled = false
    hum:ChangeState(Enum.HumanoidStateType.Landed)
end
