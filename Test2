    local data = bundles[bundleName]
    if data then
        for _, v in pairs(hum:GetPlayingAnimationTracks()) do v:Stop(0) end

        for name, id in pairs(data) do
            if animate:FindFirstChild(name) then
                local animObj = animate[name]:FindFirstChildOfClass("Animation")
                if animObj then animObj.AnimationId = id end
            end
        end

        animate.Disabled = true
        task.wait(0.1)
        animate.Disabled = false
        
        hum:ChangeState(Enum.HumanoidStateType.Jumping)
    end
